<odoo>
    <data>
        <record id="contacts.menu_contacts" model="ir.ui.menu">
            <field name="action" eval="False"/>
            <field name="active" eval="True"/>
            <field name="groups_id"
                   eval="[(6, 0, [ref('base.group_erp_manager'), ref('s2pc_securite.group_menu_contact_access')])]"/>
            <field name="name">Contacts</field>
            <field name="parent_id" eval="False"/>
            <field name="sequence">20</field>
            <field name="web_icon">contacts,static/description/icon.png</field>
        </record>

        <record id="hr.menu_hr_root" model="ir.ui.menu">
            <field name="action" eval="False"/>
            <field name="active" eval="True"/>
            <field name="groups_id"
                   eval="[(6, 0, [ref('base.group_erp_manager'), ref('hr.group_hr_user'), ref('s2pc_securite.group_menu_employee_access')])]"/>
            <field name="name">Employees</field>
            <field name="parent_id" eval="False"/>
            <field name="sequence">185</field>
            <field name="web_icon">hr,static/description/icon.png</field>
        </record>

        <record id="mass_mailing.mass_mailing_menu_root" model="ir.ui.menu">
            <field name="action" eval="False"/>
            <field name="active" eval="True"/>
            <field name="groups_id"
                   eval="[(6, 0, [ref('base.group_erp_manager'), ref('mass_mailing.group_mass_mailing_user'), ref('s2pc_securite.group_menu_email_marketing_access')])]"/>
            <field name="name">Employees</field>
            <field name="parent_id" eval="False"/>
            <field name="sequence">185</field>
            <field name="web_icon">mass_mailing,static/description/icon.png</field>
        </record>

        <record id="maintenance.menu_maintenance_title" model="ir.ui.menu">
            <field name="action" eval="False"/>
            <field name="active" eval="True"/>
            <field name="groups_id"
                   eval="[(6, 0, [ref('base.group_erp_manager'), ref('maintenance.group_equipment_manager'), ref('s2pc_securite.group_menu_maintenance_access')])]"/>
            <field name="name">Employees</field>
            <field name="parent_id" eval="False"/>
            <field name="sequence">185</field>
            <field name="web_icon">maintenance,static/description/icon.png</field>
        </record>


    </data>
</odoo>